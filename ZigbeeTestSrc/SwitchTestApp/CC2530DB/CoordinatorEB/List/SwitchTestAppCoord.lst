    418          /*********************************************************************
    419           * @fn      SwitchTestApp_ProcessZDOMsgs()
    420           *
    421           * @brief   Process response messages
    422           *
    423           * @param   none
    424           *
    425           * @return  none
    426           */
    427          static void SwitchTestApp_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    428          {
    429            switch ( inMsg->clusterID )
    430            {
    431              case End_Device_Bind_rsp:
    432                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    433                {
    434                  // Light LED
    435                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    436                }
    437          #if defined( BLINK_LEDS )
    438                else
    439                {
    440                  // Flash LED to show failure
    441                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    442                }
    443          #endif
    444                break;
    445          
    446              case Match_Desc_rsp:
    447                {
    448                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    449                  if ( pRsp )
    450                  {
    451                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    452                    {
    453                      SwitchTestApp_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    454                      SwitchTestApp_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    455                      // Take the first endpoint, Can be changed to search through endpoints
    456                      SwitchTestApp_DstAddr.endPoint = pRsp->epList[0];
    457          
    458                      // Light LED
    459                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    460                    }
    461                    osal_mem_free( pRsp );
    462                  }
    463                }
    464                break;
    465            }
    466          }
