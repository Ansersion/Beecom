    700                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    701                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    702                  break;
    703                }
    704                
    705                default:
    706                  break;  /* Ignore unknown command */    
