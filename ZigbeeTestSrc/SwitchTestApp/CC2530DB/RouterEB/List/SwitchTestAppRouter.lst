    399          /*********************************************************************
    400           * @fn      SwitchTestApp_ProcessZDOMsgs()
    401           *
    402           * @brief   Process response messages
    403           *
    404           * @param   none
    405           *
    406           * @return  none
    407           */
    408          static void SwitchTestApp_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    409          {
    410            switch ( inMsg->clusterID )
    411            {
    412              case End_Device_Bind_rsp:
    413                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    414                {
    415                  // Light LED
    416                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    417                }
    418          #if defined( BLINK_LEDS )
    419                else
    420                {
    421                  // Flash LED to show failure
    422                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    423                }
    424          #endif
    425                break;
    426          
    427              case Match_Desc_rsp:
    428                {
    429                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    430                  if ( pRsp )
    431                  {
    432                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    433                    {
    434                      SwitchTestApp_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    435                      SwitchTestApp_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    436                      // Take the first endpoint, Can be changed to search through endpoints
    437                      SwitchTestApp_DstAddr.endPoint = pRsp->epList[0];
    438          
    439                      // Light LED
    440                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    441                    }
    442                    osal_mem_free( pRsp );
    443                  }
    444                }
    445                break;
    446            }
    447          }
