    449          /*********************************************************************
    450           * @fn      SwitchTestApp_HandleKeys
    451           *
    452           * @brief   Handles all key events for this device.
    453           *
    454           * @param   shift - true if in shift/alt.
    455           * @param   keys - bit field for key events. Valid entries:
    456           *                 HAL_KEY_SW_4
    457           *                 HAL_KEY_SW_3
    458           *                 HAL_KEY_SW_2
    459           *                 HAL_KEY_SW_1
    460           *
    461           * @return  none
    462           */
    463          static void SwitchTestApp_HandleKeys( uint8 shift, uint8 keys )
    464          {
    465            zAddrType_t dstAddr;
    466          
    467            // Shift is used to make each button/switch dual purpose.
    468            if ( shift )
    469            {
    470              if ( keys & HAL_KEY_SW_1 )
    471              {
    472              }
    473              if ( keys & HAL_KEY_SW_2 )
    474              {
    475              }
    476              if ( keys & HAL_KEY_SW_3 )
    477              {
    478              }
    479              if ( keys & HAL_KEY_SW_4 )
    480              {
    481              }
    482            }
    483            else
    484            {
    485              if ( keys & HAL_KEY_SW_1 )
    486              {
    487                // Since SW1 isn't used for anything else in this application...
    488          #if defined( SWITCH1_BIND )
    489                // we can use SW1 to simulate SW2 for devices that only have one switch,
    490                keys |= HAL_KEY_SW_2;
    491          #elif defined( SWITCH1_MATCH )
    492                // or use SW1 to simulate SW4 for devices that only have one switch
    493                keys |= HAL_KEY_SW_4;
    494          #endif
    495              }
    496          
    497              if ( keys & HAL_KEY_SW_2 )
    498              {
    499                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    500          
    501                // Initiate an End Device Bind Request for the mandatory endpoint
    502                dstAddr.addrMode = Addr16Bit;
    503                dstAddr.addr.shortAddr = 0x0000; // Coordinator
    504                ZDP_EndDeviceBindReq( &dstAddr, NLME_GetShortAddr(),
    505                                      SwitchTestApp_epDesc.endPoint,
    506                                      SwitchTestApp_PROFID,
    507                                      SwitchTestApp_MAX_CLUSTERS, (cId_t *)SwitchTestApp_ClusterList,
    508                                      SwitchTestApp_MAX_CLUSTERS, (cId_t *)SwitchTestApp_ClusterList,
    509                                      FALSE );
    510              }
    511          
    512              if ( keys & HAL_KEY_SW_3 )
    513              {
    514              }
    515          
    516              if ( keys & HAL_KEY_SW_4 )
    517              {
    518                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    519                // Initiate a Match Description Request (Service Discovery)
    520                dstAddr.addrMode = AddrBroadcast;
    521                dstAddr.addr.shortAddr = NWK_BROADCAST_SHORTADDR;
