    691                {
    692                  userQueue_t outMsg;
    693          
    694                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    695                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    696                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    697                  break;
    698                }
    699                
    700                default:
    701                  break;  /* Ignore unknown command */    
    702              }
    703            }
